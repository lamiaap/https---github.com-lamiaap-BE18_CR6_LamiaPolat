{% extends 'base.html.twig' %}

{% block title %}Event{% endblock %}

{% block body %}
    <h1>Event</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ test.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ test.name }}</td>
            </tr>
            <tr>
                <th>Date</th>
                <td>{{ test.date ? test.date|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>Description</th>
                <td>{{ test.description }}</td>
            </tr>
            <tr>
                <th>Image</th>
                <td>{{ test.image }}</td>
            </tr>
            <tr>
                <th>Capacity</th>
                <td>{{ test.Capacity }}</td>
            </tr>
            <tr>
                <th>Contact</th>
                <td>{{ test.contact }}</td>
            </tr>
            <tr>
                <th>Phone</th>
                <td>{{ test.phone }}</td>
            </tr>
            <tr>
                <th>Address</th>
                <td>{{ test.address }}</td>
            </tr>
            <tr>
                <th>Url</th>
                <td>{{ test.url }}</td>
            </tr>
            <tr>
                <th>Type</th>
                <td>{{ test.Type }}</td>
            </tr>
        </tbody>
    </table>

    <a class="btn btn-warning"href="{{ path('app_test_index') }}">back to list</a>

    <a class="btn btn-info" href="{{ path('app_test_edit', {'id': test.id}) }}">edit</a>

    {{ include('test/_delete_form.html.twig') }}
{% endblock %}
